<?xml version="1.0" encoding="utf-8"?>
<inset android:inset="0.0dip"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ripple android:color="?colorPrimary">
        <item>
            <selector>
                <item android:state_enabled="false">
                    <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="#ffbcbcbc" />
                </item>
                <item android:state_focused="true">
                    <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="#00000000" />
                </item>
                <item android:state_pressed="true">
                    <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="#00000000" />
                </item>
                <item>
                    <nine-patch android:src="@drawable/tw_textfield_default_mtrl_alpha" android:tint="#ff7f7f7f" />
                </item>
            </selector>
        </item>
        <item android:id="@id/mask" android:drawable="@drawable/tw_textfield_activated_mtrl_alpha" />
    </ripple>
</inset>