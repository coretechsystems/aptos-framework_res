<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:gravity="center" android:src="@drawable/tw_btn_switch_mtrl" android:tint="@color/tw_switch_dim_mtrl" android:tintMode="multiply" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:gravity="center" android:src="@drawable/tw_btn_switch_mtrl" android:tint="@color/tw_switch_dim_mtrl" android:tintMode="multiply" />
    </item>
    <item android:state_checked="true" android:id="@id/on">
        <bitmap android:gravity="center" android:src="@drawable/tw_btn_switch_mtrl" android:tint="@color/tw_switch_on_mtrl" android:tintMode="multiply" />
    </item>
    <item android:id="@id/off">
        <bitmap android:gravity="center" android:src="@drawable/tw_btn_switch_mtrl" android:tint="@color/tw_switch_off_mtrl" android:tintMode="multiply" />
    </item>
</animated-selector>